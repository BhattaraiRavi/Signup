<div class="container">
  <div class="row clearfix well">
    <!-- Carousel -->
      <div id="mySlide" class="carousel slide" data-ride="carousel" style="width: 960px; height: 450px; margin: 0 auto">
      <!-- Indicators -->
      <ol class="carousel-indicators">
          <li data-target="#mySlide" data-slide-to="0" class="active"></li>
          <li data-target="#mySlide" data-slide-to="1"></li>
          <li data-target="#mySlide" data-slide-to="2"></li>
      </ol>
      <!-- Wrapper for slides -->
      <div class="carousel-inner">
          <div class="item active">
            <img src="astronomy.png" alt="First slide" style="width: 960px; height: 450px;">
          </div>
          <div class="item">
            <img src="ruby_bootcamp.jpg" alt="Second slide" style="width: 960px; height: 450px;">
          </div>
          <div class="item">
            <img src="biologistics_lightning_talks.jpg" alt="Third slide" style="width: 960px; height: 450px;">
          </div>
      </div>
      <!-- Controls -->
    </div><!-- /carousel -->
  </div>
</div>

<script>
  $(function(){
    $('#mySlide').carousel();
  });
</script>
